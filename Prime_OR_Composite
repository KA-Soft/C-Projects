//Prime OR Composite

#include<iostream>
using namespace std;

int main()
{
  int n;

  cout<<"Enter a number : ";
  cin>>n;

  bool check=false;
  for(int i=2; i<=n/2; i++)
  {
    if(n%i==0)
      {
        check=true;
        break;
      }
  }
  if(check)
    cout<<"\n"<<n<<" is composite number.";
  else
    cout<<"\n"<<n<<" is prime number.";

  return 0;

}
