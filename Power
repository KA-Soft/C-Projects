//power without pow() function
#include<iostream>
using namespace std;

int main()
{
    int a,b;

    cout<<"Enter first number : ";
    cin>>a;//2
    cout<<"\nEnter second number : "; 
    cin>>b;//3

    int r=1;
    for(int i=0; i<b;i++)//0->1->3->false
    {
        r=r*a;//2->4->8   
    }
    cout<<"\n"<<r;//8

    return 0;
}
